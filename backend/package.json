{
    "name": "backend",
    "version": "1.0.0",
    "description": "backend is the ultimate backend solution",
    "main": "src/server.js",
    "scripts": {
        "setup": "npm install && npm update",
        "tsc": "tsc",
        "build": "npm run tsc",
        "start": "ts-node-dev --transpile-only ./src/server.ts",
        "dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
        "prod": "tsc && node ./build/src/server.js",
        "test": "mocha -r ts-node/register tests/**/*.test.ts",
        "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test"
    },
    "keywords": [
        "express",
        "backend",
        "backend"
    ],
    "license": "UNLICENSED",
    "dependencies": {
        "@types/express": "^4.17.1",
        "cheerio": "^1.0.0-rc.5",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "express-custom-error": "^1.8.4",
        "has-keys": "^0.1.0",
        "helmet": "^3.21.2",
        "http-status": "^1.3.2",
        "mandatoryenv": "^1.1.1",
        "module-alias": "^2.2.2",
        "morgan": "^1.9.1",
        "phin": "^3.5.1",
        "puppeteer": "^5.5.0",
        "queue": "^6.0.2",
        "rotating-file-stream": "^1.4.6",
        "typescript": "^3.6.3"
    },
    "devDependencies": {
        "@types/mocha": "^8.2.0",
        "mocha": "^8.2.1",
        "nyc": "^15.1.0",
        "ts-node": "^9.1.1",
        "ts-node-dev": "^1.1.1"
    }
}
